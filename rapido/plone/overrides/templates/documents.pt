<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="rapido.plone">
<body>

<metal:main fill-slot="content-core">
  <metal:content-core define-macro="content-core">

    <table class="plain">
      <tr><th>UID</th><th>DOCID</th></tr>
      <tal:documents tal:repeat="doc view/documents">
        <tr>
          <td>
            <a tal:attributes="href doc/url" tal:content="doc/uid">uid</a>
          </td>
          <td>
            <a tal:attributes="href doc/url" tal:content="python:doc.get_item('docid')">docid</a>
          </td>
        </tr>
      </tal:documents>
    </table>

  </metal:content-core>
</metal:main>

</body>
</html>

